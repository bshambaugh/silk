{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "baseUrl": ".",
        "paths": {
            "@gui-elements/*": ["./src/libs/gui-elements/*"]
        },
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "resolveJsonModule": true,
    }
}
